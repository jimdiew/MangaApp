{
  "_version": "1.32.0",
  "sap.app": {
    "id": "com.pampa.zacademiaej",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "1.0.0"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "ach": "ach",
    "dataSources": {
      "mainService": {
        "uri": "/sap/opu/odata/sap/ZOS_ACADEMIA_LUUC3_SRV",
        "type": "OData",
        "settings": {
          "odataVersion": "2.0",
          "localUri": "localService/metadata.xml"
        }
      }
		}
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "sap-icon://task",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },

  "sap.ui5": {
    "flexEnabled": false,
    "rootView": {
      "viewName": "com.pampa.zacademiaej.view.Practica",
      "type": "XML",
      "async": true,
      "id": "Practica"
    },
    "dependencies": {
      "minUI5Version": "1.66.0",
      "libs": {
        "sap.ui.core": {},
        "sap.m": {},
        "sap.ui.layout": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "com.pampa.zacademiaej.i18n.i18n"
        }
      },
        "": {
            "dataSource": "mainService",
            "preload": true
        }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "com.pampa.zacademiaej.view",
        "controlAggregation": "pages",
        "controlId": "App",
        "clearControlAggregation": false
      },
      "routes": [
        {
          "name": "RoutePractica",
          "pattern": "",
          "target": ["TargetPractica"]
        },
        {
          "name": "platos",
          "pattern": "Platos/{Cuil}",
          "target": ["TargetPlatos"]
        }
      ],
      "targets": {
        "TargetPractica": {
          "viewType": "XML",
          "transition": "flip",
          "viewName": "Practica"
        },
        "TargetPlatos": {
          "viewType": "XML",
          "transition": "flip",
          "viewName": "Platos"
        }
      }
    }
  }
}
